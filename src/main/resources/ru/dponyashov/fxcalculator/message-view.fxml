<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="100.0" maxWidth="300.0" minHeight="100.0" minWidth="300.0" prefHeight="100.0" prefWidth="300.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/22" fx:controller="ru.dponyashov.fxcalculator.fxcontroller.MessageFXController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="messageText" alignment="TOP_CENTER" prefHeight="16.0" prefWidth="302.0" text="Сообщение" textAlignment="CENTER" />
    <Button contentDisplay="BOTTOM" onAction="#onButtonClick" text="OK!" textAlignment="CENTER" />
</VBox>
